{
    "compilerOptions": {
        "outDir": "lib/",
        "sourceMap": false,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "noEmitOnError": true,
        "baseUrl": "src",
        "paths": {
            "redux": ["node_modules/redux-next"]
        },
        "module": "commonjs",
        "target": "es5",
        "lib": [ "ES7", "DOM"],
        "jsx": "react"
    },
    "include": [ "src/**/*"]
}
